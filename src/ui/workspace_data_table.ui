    font: 11pt &quot;Consolas&quot;;
}

/* Sorok alap */
QTreeWidget::item {
    height: 28px;
    padding: 4px;
}

/* Hover */
QTreeWidget::item:hover {
    background-color: #2b2b2b;
}

/* Kijelölt sor */
QTreeWidget::item:selected {
    background-color: #3d6aff;
    color: white;
    border: none;
}

/* Kijelölt + hover */
QTreeWidget::item:selected:hover {
    background-color: #5278ff;
}

/* Checkbox — ALAP (nincs kép, gyönyörű QSS kocka) */
QTreeWidget::indicator {
    width: 16px;
    height: 16px;
    border-radius: 3px;
    border: 1px solid #666;
    background: #222;
}

/* Hover checkbox */
QTreeWidget::indicator:hover {
    border: 1px solid #aaa;
}

/* Unchecked */
QTreeWidget::indicator:unchecked {
    background: #2a2a2a;
}

/* Checked */
QTreeWidget::indicator:checked {
    background-color: #3d6aff;
    border: 1px solid #3d6aff;
}

/* CHECKBOX a kijelölt soron → világosabb legyen hogy látszódjon */
QTreeWidget::item:selected QTreeWidget::indicator {
    background: #eef1ff;
    border: 1px solid white;
}

/* CHECKED a kijelölt soron → kék maradjon de világosabb */
QTreeWidget::item:selected QTreeWidget::indicator:checked {
    background-color: #ffffff;
    border: 1px solid white;
}

/* Header */
QHeaderView::section {
    background-color: #2b2b2b;
