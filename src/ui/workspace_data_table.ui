/* ====== ALAP ITEM ====== */
QTreeWidget::item {
    height: 28px;
    padding: 4px;
    border: none;
}

/* ====== D√ÅTUM KATEG√ìRIA (has-children) ====== */
QTreeWidget::item:has-children {
    font-weight: bold;
    color: #bfbfbf;            /* kiss√© vil√°gosabb mint a sima text */
}

/* Hover a kateg√≥ri√°ra */
QTreeWidget::item:has-children:hover {
    background-color: #2b2b2b;
}

/* ====== F√ÅJL ELEMEK (nincs children) ====== */
QTreeWidget::item:!has-children {
    font-weight: normal;
    color: #dcdcdc;
}

/* Hover f√°jl elemekre */
QTreeWidget::item:!has-children:hover {
    background-color: #2b2b2b;
}

/* ====== SELECTED ====== */
QTreeWidget::item:selected {
    background-color: #3d6aff;
    color: white;
}

QTreeWidget::item:selected:hover {
    background-color: #5278ff;
}

/* ====== CHECKBOXOK ====== */
QTreeWidget::indicator {
    width: 16px;
    height: 16px;
    border-radius: 3px;
    border: 1px solid #666;
    background: #222;
}

QTreeWidget::indicator:hover {
    border: 1px solid #aaa;
}

QTreeWidget::indicator:unchecked {
    background: #2a2a2a;
}

QTreeWidget::indicator:checked {
    background-color: #3d6aff;
    border: 1px solid #3d6aff;
}

/* Kijel√∂lt sor checkbox ‚Äî legyen j√≥l l√°that√≥ */
QTreeWidget::item:selected QTreeWidget::indicator {
    background: #eef1ff;
    border: 1px solid white;
}

