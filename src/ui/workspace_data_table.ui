}

/* Scrollbar */
QScrollBar:vertical {
    width: 12px;
    background: #1e1e1e;
}

QScrollBar::handle:vertical {
    background: #3a3a3a;
    min-height: 20px;
    border-radius: 6px;
}

QScrollBar::handle:vertical:hover {
    background: #505050;
}

QScrollBar::add-line,
